{
    "ipAddress": {
      "networkName": "dcos"
    },
    "constraints": [
        [
            "hostname",
            "LIKE",
            "192.168.5.4"
        ]
    ],
    "backoffSeconds": 20,
    "container": {
        "docker": {
            "image": "us.gcr.io/fathm-ci/mattermost:3.6",
            "network": "USER",
            "portMappings": [
                {
                    "containerPort": 8065,
                    "hostPort": 0
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 1,
    "id": "mattermost",
    "mem": 2048,
    "acceptedResourceRoles": ["slave_public"],
    "labels":{
      "HAPROXY_GROUP":"external",
      "HAPROXY_0_VHOST":"chat.fathm.io"
    },
    "uris": [
       "file:///etc/docker.tar.gz"
    ]
}
